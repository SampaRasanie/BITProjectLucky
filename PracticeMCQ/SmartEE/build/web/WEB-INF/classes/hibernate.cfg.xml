<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/practicemcqdb?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="pojo/PracticeQuestion.hbm.xml"/>
    <mapping resource="pojo/EnumUserType.hbm.xml"/>
    <mapping resource="pojo/Answerdescription.hbm.xml"/>
    <mapping resource="pojo/Question.hbm.xml"/>
    <mapping resource="pojo/Answers.hbm.xml"/>
    <mapping resource="pojo/Account.hbm.xml"/>
    <mapping resource="pojo/EnumUserStatus.hbm.xml"/>
    <mapping resource="pojo/QuestionSet.hbm.xml"/>
    <mapping resource="pojo/PracticeQuestionSet.hbm.xml"/>
    <mapping resource="pojo/EnumSubject.hbm.xml"/>
    <mapping resource="pojo/AnswersHistory.hbm.xml"/>
    <mapping resource="pojo/EnumSecurityQuestions.hbm.xml"/>
    <mapping resource="pojo/User.hbm.xml"/>
    <mapping resource="pojo/EnumAccountStatus.hbm.xml"/>
    <mapping resource="pojo/EnumQuestionStatus.hbm.xml"/>
    <mapping resource="pojo/EnumSections.hbm.xml"/>
    <mapping resource="pojo/EnumQuestionSetStatus.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
