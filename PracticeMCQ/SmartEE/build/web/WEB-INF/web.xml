<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>servlets.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emailchack</servlet-name>
        <servlet-class>servlets.emailchack</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkusertype</servlet-name>
        <servlet-class>servlets.checkusertype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checksubject</servlet-name>
        <servlet-class>servlets.checksubject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addpaper</servlet-name>
        <servlet-class>servlets.Addpaper</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addpaper_en</servlet-name>
        <servlet-class>servlets.Addpaper_en</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Answered</servlet-name>
        <servlet-class>servlets.Answered</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addSecu</servlet-name>
        <servlet-class>servlets.addSecu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>servlets.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkq</servlet-name>
        <servlet-class>servlets.checkq</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resetpass</servlet-name>
        <servlet-class>servlets.resetpass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deactive</servlet-name>
        <servlet-class>servlets.deactive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servlets.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emailchack</servlet-name>
        <url-pattern>/emailchack</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkusertype</servlet-name>
        <url-pattern>/checkusertype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checksubject</servlet-name>
        <url-pattern>/checksubject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addpaper</servlet-name>
        <url-pattern>/Addpaper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addpaper_en</servlet-name>
        <url-pattern>/Addpaper_en</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Answered</servlet-name>
        <url-pattern>/Answered</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addSecu</servlet-name>
        <url-pattern>/addSecu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkq</servlet-name>
        <url-pattern>/checkq</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resetpass</servlet-name>
        <url-pattern>/resetpass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deactive</servlet-name>
        <url-pattern>/deactive</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/404.jsp</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/500.jsp</location>
    </error-page>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
